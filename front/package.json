  "homepage": "https:turpin-jonathan.fr",
  "license": "MIT",
  "author": "Jonathan TURPIN",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "prettier --write '**/*.{js,css,scss,html}' && eslint .",
    "lint:fix": "prettier --write '**/*.{js,css,scss,html}' && eslint . --fix",
    "lint:css": "prettier --write '**/*.{css,scss}' && stylelint '**/*.{css,scss}'",
    "lint:css:fix": "prettier --write '**/*.{css,scss}' && stylelint '**/*.{css,scss}' --fix",
    "lint:all": "npm run lint && npm run lint:css",
    "lint:all:fix": "npm run lint:fix && npm run lint:css:fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^6.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.6.0",
    "@fortawesome/pro-light-svg-icons": "^6.6.0",
    "@fortawesome/pro-regular-svg-icons": "^6.6.0",
    "@fortawesome/pro-solid-svg-icons": "^6.6.0",
    "@fortawesome/pro-thin-svg-icons": "^6.6.0",
    "@fortawesome/sharp-duotone-solid-svg-icons": "^6.6.0",
    "@fortawesome/sharp-light-svg-icons": "^6.6.0",
    "@fortawesome/sharp-regular-svg-icons": "^6.6.0",
    "@fortawesome/sharp-solid-svg-icons": "^6.6.0",
    "@fortawesome/sharp-thin-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@vueuse/core": "^11.2.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.15.1",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "sass": "^1.80.6",
    "start-server-and-test": "^2.0.8",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.8.1",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
