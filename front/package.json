{
  "name": "front",
  "description": "The portfolio of Jonathan TURPIN, web developer.",
  "keywords": [
    "jonathan",
    "turpin",
    "symfony",
    "developer",
    "portfolio",
    "web",
    "vue",
    "vite",
    "typescript",
