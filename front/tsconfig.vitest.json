{
  "extends": "./tsconfig.app.json",
  "exclude": [],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
    "lib": [],
    "types": [
      "node",
      "jsdom"
    ],
    "paths": {
      "#/*": ["src/*"],
      "#components/*": ["src/components/*"],
      "#router/*": ["src/router/*"],
      "#store/*": ["src/stores/*"],
      "#views/*": ["src/views/*"],
      "#assets/*": ["src/assets/*"],
      "#utils/*": ["src/utils/*"],
      "#styles/*": ["src/styles/*"],
      "#constants/*": ["src/constants/*"],
      "#enums/*": ["src/enums/*"],
      "#composables/*": ["src/composables/*"]
    }
  },
  "include": [
    "src/**/*.d.ts",
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "font-awesome.ts"
  ]
}