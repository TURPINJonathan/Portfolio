{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "env.d.ts"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "#/*": [
        "./src"
      ],
      "#components/*": [
        "./src/components/*"
      ],
      "#router/*": [
        "./src/router/*"
      ],
      "#store/*": [
        "./src/stores/*"
      ],
      "#views/*": [
        "./src/views/*"
      ],
      "#assets/*": [
        "./src/assets/*"
      ],
      "#styles/*": [
        "./src/styles/*"
      ],
      "#utils/*": [
        "./src/utils/*"
      ],
      "#constants/*": [
        "./src/constants/*"
      ],
      "#enums/*": [
        "./src/enums/*"
      ],
      "#composables/*": [
        "./src/composables/*"
      ],
    }
  }
}